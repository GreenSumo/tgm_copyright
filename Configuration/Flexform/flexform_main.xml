<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Settings</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.displayDuplicateImages>
                        <TCEforms>
                            <label>Display multiple used images more than once?</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.displayDuplicateImages>
                    <settings.onlyCurrentPage>
                        <TCEforms>
                            <label>Display only copyright images of the current page?</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.onlyCurrentPage>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>